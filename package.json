    "express": "^4.17.1",
    "http-proxy-middleware": "^1.0.5",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.22",
    "multer": "^1.4.2",
    "nodemon": "^2.0.4",
    "path-parser": "^6.1.0",
    "react-gradient-text": "^0.1.0",
    "socket.io": "^2.5.0",
    "url": "^0.11.0"
